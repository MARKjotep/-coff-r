{
  "name": "@coff-r/oven",
  "version": "0.0.2",
  "description": "Bun bundler utility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dirs": "for dir in ./src ./dist ./test ./util ./types; do mkdir -p $dir; done",
    "ts": "cp ../../misc/tsconfig.json ./;",
    "init": "bun run dirs && bun run ts;"
  },
  "keywords": [
    "js",
    "ts",
    "bun"
  ],
  "author": "MARKjotep",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MARKjotep/-coff-r.git"
  },
  "bugs": {
    "url": "https://github.com/MARKjotep/-coff-r/issues"
  },
  "homepage": "https://github.com/MARKjotep/-coff-r#readme",
  "dependencies": {
    "rollup": "^4.53.1",
    "rollup-plugin-dts": "^6.2.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
